{
  "name": "@pro3/database",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "drizzle-seed": "^0.3.1"
  },
  "nx": {
    "tags": [
      "type:shared",
      "scope:server/database"
    ],
    "targets": {
      "docker-compose-up": {
        "executor": "nx:run-commands",
        "options": {
          "command": "docker-compose up -d"
        }
      },
      "docker-compose-down": {
        "executor": "nx:run-commands",
        "options": {
          "command": "docker-compose down"
        }
      },
      "genMigration": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/server/database",
          "command": "drizzle-kit generate"
        }
      },
      "migrate": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/server/database",
          "command": "drizzle-kit migrate"
        }
      },
      "pull": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/server/database",
          "command": "drizzle-kit pull"
        }
      },
      "push": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "docker-compose-up"
        ],
        "configurations": {
          "force": {
            "command": "drizzle-kit push --force"
          },
          "development": {
            "command": "drizzle-kit push"
          }
        },
        "options": {
          "cwd": "libs/server/database"
        }
      },
      "drop": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/server/database",
          "command": "drizzle-kit drop"
        }
      },
      "studio": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/server/database",
          "command": "drizzle-kit studio"
        }
      },
      "seed": {
        "dependsOn": [
          "push:force"
        ],
        "executor": "nx:run-commands",
        "options": {
          "cwd": "libs/server/database",
          "command": "npx tsx ./src/seed.ts"
        }
      },
      "init": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "nx run @pro3/database:seed"
          ],
          "parallel": false
        },
        "dependsOn": [
          {
            "target": "push",
            "params": "forward"
          }
        ],
        "configurations": {
          "force": {},
          "development": {}
        }
      }
    }
  }
}